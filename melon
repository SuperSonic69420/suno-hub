	-- Spawns

	spawn(function() -- AutoKey
		while wait() do
			if States.AutoKey then
				pcall(function()
					if not s.Players.LocalPlayer.Backpack:FindFirstChild("Key card") and not s.Players.LocalPlayer.Character:FindFirstChild("Key card") then
						pcall(function()
							local savedcf = GetPos()
							local savedcamcf = GetCamPos()
							repeat wait(0.2)
								game.Players.LocalPlayer.Character.Humanoid.Health = 0
								workspace.Remote.loadchar:InvokeServer(nil, BrickColor.new("Bright blue").Name)
							until workspace.Prison_ITEMS.single:FindFirstChild("Key card")
							wait()
							local Item = game.Players.LocalPlayer:FindFirstChild("Backpack"):FindFirstChild("Key card") or game.Players.LocalPlayer.Character:FindFirstChild("Key card")
							wait(0.2)
							workspace.Remote.loadchar:InvokeServer(nil, BrickColor.new("Medium green").Name)
							game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = savedcf
							workspace.CurrentCamera.CFrame = savedcamcf
							workspace.Remote.ItemHandler:InvokeServer(workspace.Prison_ITEMS.single["Key card"].ITEMPICKUP)
						end)
					elseif s.Players.LocalPlayer.Backpack:FindFirstChild("Key card") and not s.Players.LocalPlayer.Character:FindFirstChild("Key card") then
						return
					elseif not s.Players.LocalPlayer.Backpack:FindFirstChild("Key card") and s.Players.LocalPlayer.Character:FindFirstChild("Key card") then
						return
					end
				end)
			end
		end
	end)
	
