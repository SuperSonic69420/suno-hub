				s.Players.LocalPlayer.Character.Humanoid.WalkSpeed = v
			end,
			Min = 50,
			Max = 250,
			Def = 50
		})
	end;

	local Misc = Library.New({
		Title = "Misc";
	}); do
		Misc.Button({
			Text = "Infinite Cash",
			Callback = function()
				loadstring(game:HttpGetAsync('https://raw.githubusercontent.com/SuperSonic69420/bruh-idk/main/AHHAHAHAHHAAHPENIS'))()
			end;
		});
	end

	local Credits = Library.New({
		Title = "Credits"
	}); do
		Credits.Button({
			Text = "Ui Lib: Material Ui"
		});
		Credits.Button({
			Text = "Script Dev: "..dev
		});
		Credits.Button({
			Text = dev.."'s Server",
			Callback = function()
				local http = game:GetService('HttpService') 
				if toClipboard then
					toClipboard('https://discord.com/invite/JVZbK646gW')
				end
				local req = syn and syn.request or http and http.request or http_request or fluxus and fluxus.request or getgenv().request or request
				if req then
					req({
						Url = 'http://127.0.0.1:6463/rpc?v=1',
						Method = 'POST',
						Headers = {
							['Content-Type'] = 'application/json',
							Origin = 'https://discord.com'
